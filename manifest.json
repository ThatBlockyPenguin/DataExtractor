{
  "name": "Data Extractor",
  "description": "Toy extension to extract metadata from a web page",
  "version": "0.1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
